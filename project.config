[project]
	description = Puppet modules to deploy MidoNet on top of OpenStack.
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group midonet-admins
	owner = group user/Jaume Devesa (jdevesa)
[access "refs/heads/*"]
	read = group Registered Users
	push = +force group midonet-installers
	pushMerge = group midonet-installers
	exclusiveGroupPermissions = forgeCommitter push pushMerge label-Code-Review label-Verified labelAs-Verified removeReviewer submit
	forgeCommitter = group midonet-installers
	label-Code-Review = -2..+2 group midonet-installers
	label-Code-Review = -1..+1 group Registered Users
	label-Verified = -1..+1 group midonet-bots
	label-Verified = -1..+1 group midonet-installers
	labelAs-Verified = -1..+1 group midonet-installers
	removeReviewer = group midonet-installers
	submit = group midonet-installers
[access "refs/tags/*"]
	push = +force group midonet-installers
	pushTag = +force group midonet-installers
	pushSignedTag = group midonet-installers
	exclusiveGroupPermissions = push pushTag pushSignedTag
	read = group Registered Users
[receive]
	requireSignedOffBy = false
	requireChangeId = true
	createNewChangeForAllNotInTarget = false
[access "refs/drafts/*"]
	read = group Registered Users
	push = group Registered Users
[access "refs/publish/*"]
	read = group Registered Users
	push = +force group Registered Users
