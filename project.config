[project]
	description = Puppet modules to deploy MidoNet on top of OpenStack.
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group midonet-admins
	owner = group user/Jaume Devesa (jdevesa)
[access "refs/heads/*"]
	read = group Registered Users
	push = group midonet-installers
	pushMerge = group midonet-installers
	exclusiveGroupPermissions = push pushMerge
[access "refs/tags/*"]
	push = +force group user/Jaume Devesa (jdevesa)
	pushTag = group user/Jaume Devesa (jdevesa)
	pushSignedTag = group user/Jaume Devesa (jdevesa)
[receive]
	requireSignedOffBy = false
	requireChangeId = true
	createNewChangeForAllNotInTarget = false
[access "refs/drafts/*"]
	read = group Registered Users
	push = group Registered Users
[access "refs/pubish/*"]
	read = group Registered Users
	push = group Registered Users
